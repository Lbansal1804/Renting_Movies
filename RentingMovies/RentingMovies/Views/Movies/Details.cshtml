@model RentingMovies.Models.Movie
@{
    ViewBag.Title = "Movie_Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Date Added: @Model.DateAdded.ToLongDateString()</li>
    <li>Released Date: @Model.ReleaseDate.ToLongDateString()</li>
    <li>Number In Stock: @Model.NumberInStock</li>
    <li>Genre: @Model.General.Name</li>
</ul>

